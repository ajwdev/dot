{
	"runtime.version": "LuaJIT",
	"runtime.path": [
		"lua/?.lua",
		"lua/?/init.lua"
	],
	"diagnostics.globals": [
		"vim"
	],
	"workspace.library": [
		"$VIMRUNTIME",
		"${3rd}/luv/library"
	],
	"workspace.checkThirdParty": false,
	"telemetry.enable": false,
	"format.enable": true,
	"format.defaultConfig": {
		"indent_style": "space",
		"indent_size": "2",
		"continuation_indent": "2"
	}
}
