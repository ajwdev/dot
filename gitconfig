[core]
  editor = /usr/bin/vim
  excludesfile = /Users/andrew/.gitignore_global
[user]
  name = Andrew Williams
  email = williams.andrew@gmail.com
[alias]
  st = status
  co = checkout
  ls = log --oneline
  ca = commit --amend -C HEAD
  dc = diff --cached
  unstage = reset HEAD --
  bclean = "!f(){ git branch --merged master | awk '$NF !~ /master/ {print $NF}' | xargs git branch -d; }; f"
[diff]
  tool = meld
[push]
  default = simple
